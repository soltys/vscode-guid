{
	"name": "guid",
	"displayName": "guid",
	"description": "Tools for guid generation",
	"version": "0.0.1",
	"publisher": "Soltys",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.generateGuid"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.generateGuid",
			"title": "Generate Guid"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}